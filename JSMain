function calculonums()
{
    let num1 = parseInt(document.getElementById('num1').value);
    let num2 = parseInt(document.getElementById('num2').value);

    let soma = num1 + num2;
    let subtracao= num1 - num2;
    let divisao = num1/num2;
    let multiplicacao = num1 * num2;
    let exponenciacao = num1**num2;
    let resto = num1 % num2;

    console.log(num1)
    console.log(soma)

    document.getElementById('resulsoma').value = soma;
    document.getElementById('resulsub').value = subtracao;
    document.getElementById('resuldiv').value = divisao;
    document.getElementById('resulmult').value = multiplicacao;
    document.getElementById('resulexpo').value = exponenciacao;
    document.getElementById('resulrest').value = resto;
}
